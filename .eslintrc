{
  "parser": "babel-eslint",
  "extends": ["standard", "standard-react"],
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "rules": {
    "import/no-unresolved": [2, { "ignore": ["electron"] }]
  },
  "plugins": [
    "import"
  ],
  "settings": {
    "import/resolver": "webpack"
  }
}
